{
  "problemMatcher": [
    {
      "owner": "stylelint",
      "pattern": [
        {
          "regexp": "^(.*.module.css)$",
          "file": 1
        },
        {
          "regexp": "^\\s+((\\d+):(\\d+))?\\s+\\S\\s{2}(\\w+\\s.*\\w+)[ ]+([a-z-]+)$",
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5,
          "loop": true
        }
      ]
    }
  ]
}
