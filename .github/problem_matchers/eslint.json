{
    "problemMatcher": [
        {
            "owner": "eslint-stylish",
            "pattern": [
                {
                    "regexp": "^[/](srv|src|zenhub)\/(.*)$",
                    "file": 2
                },
                {
                    "regexp": "^\\s+(\\d+):(\\d+)\\s+(error|warning|info)\\s+(.*)[ ]{2,}(.*)$",
                    "line": 1,
                    "column": 2,
                    "severity": 3,
                    "message": 4,
                    "code": 5,
                    "loop": true
                }
            ]
        }
    ]
}
